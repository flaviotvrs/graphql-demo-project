type Query {
  bookById(id: ID): Book 
  bookByName(name: String): [Book]
  bookByAuthor(author_id: Int): [Book]
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
  published: String
  isbn10: String
  isbn13: String
}

type Author {
  id: ID
  firstName: String
  lastName: String
}